# SonarQube Configuration - Environment Variables
# Copy this file to .env and update with your values

# SonarQube Server Configuration
SONAR_HOST_URL=http://localhost:9000
SONAR_LOGIN=admin
SONAR_PASSWORD=admin

# Database Configuration (if using external DB)
SONAR_DB_HOST=localhost
SONAR_DB_PORT=5432
SONAR_DB_NAME=sonar
SONAR_DB_USER=sonar
SONAR_DB_PASSWORD=sonar

# Project Configuration
SONAR_PROJECT_KEY=auth
SONAR_PROJECT_NAME=Auth Service
SONAR_PROJECT_VERSION=1.0.0

# GitHub Secrets (for CI/CD)
# Add these as repository secrets in GitHub:
# SONAR_HOST_URL
# SONAR_LOGIN
# SONAR_PASSWORD
